<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>DomoMaker</title>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <script type="application/javascript" src="/assets/css/jquery-3.1.1.js"></script>
  <link rel="stylesheet" type="text/css" href="https://bootswatch.com/readable/bootstrap.min.css"/>
  <script type="application/javascript" src="assets/css/bootstrap.min.js"></script>
  <link rel="stylesheet" type="text/css" href="/assets/css/styles.css"/>
</head>
<body>
  <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-2">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">Kelsey Weinman</a>
      </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
        <ul class="nav navbar-nav">
          <li><a href="/fill">Fill</a></li>
          <li><a href="/outline">Outline</a></li>
          <li class="active"><a href="/templates">Templates</a></li>          
          <li><a href="/logout">Logout</a></li>
        </ul>
      </div>
    </div>
  </nav>
  
  <section id="templates">
  <div class="outlines" id={{csrfToken}}>
  {{#if draws}}
    {{#each draws}}
          <div class="draw" id={{this._id}}>
            {{#if this.calls}}
              {{#each this.calls}}
                <ul>
                  <li>{{this.shape}}</li>
                  <li>{{this.x}}</li>
                  <li>{{this.y}}</li>
                  <li>{{this.w}}</li>
                  <li>{{this.h}}</li>
                  <li>{{this.rad}}</li>
                  <li>{{this.stroke}}</li>
                  <li>{{this.line}}</li>
                </ul>
              {{/each}}
            {{/if}}
          </div>
    {{/each}}
  </div>
  </section>
  {{else}}
    <h3>No templates created</h3>
  {{/if}}

  
  <section id="canvases">
  </section>
  
  <canvas id="hiddenCanvas" width="700" height="500">
      Get a real browser!
  </canvas>
  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="/assets/templates.js" type="text/javascript"></script>
</body>
</html>